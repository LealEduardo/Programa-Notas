    primeira nota <input id="nota1" type="number"> <br/>
    segunda nota <input id="nota2" type="number"> <br/>
    <button onclick="media()">Calcular</button>

    <script type="text/javascript">
        function media(){
            var nota1 = parseFloat(document.getElementById("nota1").value)
            var nota2 = parseFloat(document.getElementById("nota2").value)

            
            if(nota1>nota2)
            alert("Primeira nota ta bunita hein?")
             
            else if(nota1<nota2)
            alert("Segundinha veio forte hein?")

            if(nota2=nota1)
            alert("Colou nÃ© safado")
            
        }
    </script>





